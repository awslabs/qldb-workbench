{
  "name": "qldb-workbench",
  "version": "0.0.1-alpha",
  "description": "A cross-platform visual shell for QLDB",
  "main": "build/src/index.js",
  "scripts": {
    "serve": "webpack serve --hot --config configurations/webpack.assets.config.js",
    "build": "webpack --config configurations/webpack.assets.config.js --config configurations/webpack.electron.config.js",
    "build:watch": "npm run build -- --watch",
    "start-electron": "npm run build && electron ./build/electron/index.js",
    "start": "npm run serve & npm run start-electron",
    "test": "test"
  },
  "watch": {
    "bundleAssets": {
      "patterns": [
        "assets"
      ],
      "quiet": false,
      "extensions": "png,html,ttf,css",
      "delay": 250,
      "runOnChangeOnly": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/qldb-workbench.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/awslabs/qldb-workbench/issues"
  },
  "homepage": "https://github.com/awslabs/qldb-workbench#readme",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.24.0",
    "@aws-sdk/credential-provider-cognito-identity": "^3.24.0",
    "@awsui/collection-hooks": "^1.0.18",
    "@awsui/components-react": "^3.0.200",
    "@awsui/design-tokens": "^3.0.11",
    "@awsui/global-styles": "^1.0.8",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/node": "^14.14.31",
    "ace-builds": "^1.4.12",
    "amazon-qldb-driver-js": "^0.0.1",
    "amazon-qldb-driver-nodejs": "^2.1.1",
    "aws-sdk": "^2.936.0",
    "brace": "^0.11.1",
    "crypto-browserify": "^3.12.0",
    "electron": "^13.1.4",
    "ion-js": "4.1.0",
    "jsbi": "^3.1.4",
    "logplease": "^1.2.15",
    "material-design-icons": "^3.0.1",
    "material-ui-confirm": "^2.1.1",
